# Casting tipe data (merubah satu tipe ke tipe lain)
# tipe data python ada int, float, data_string, data_boolean

# Data awal integer
print("==========integer=========")
data_integer = 9
print(data_integer, type(data_integer))

data_float = float(data_integer)
print(data_float, type(data_float))

data_string = str(data_integer)
print(data_string, type(data_string))

data_boolean = bool(data_integer)
print(data_boolean, type(data_boolean))  # akan false jika nilai int = 0


# Data awal float
print("==========float=========")
data_float = 9.3
print(data_float, type(data_float))

data_integer = int(data_float)  # akan dibulatkan kebawah
print(data_integer, type(data_integer))

data_string = str(float)
print(data_string, type(data_string))

data_boolean = bool(float)
print(data_boolean, type(data_boolean))  # akan false jika nilai float = 0

# Data awal Boolean
print("==========Boolean=========")
data_boolean = True
print(data_boolean, type(data_boolean))

data_integer = int(data_boolean)
print(data_integer, type(data_integer))

data_float = float(data_boolean)
print(data_float, type(data_float))

data_string = str(data_boolean)
print(data_string, type(data_string))

# Data awal string
print("==========String=========")
data_string = "10"
print(data_string, type(data_string))

data_integer = int(data_string)  # string harus angka
print(data_integer, type(data_integer))

data_float = float(data_string)  # string harus angka
print(data_float, type(data_float))

data_boolean = bool(data_string)  # False jika String Kosong
print(data_boolean, type(data_boolean))
